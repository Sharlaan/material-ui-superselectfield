{
  "name": "moebel-material-ui-superselectfield",
  "version": "0.1.11",
  "description": "original Material-UI's SelectField component enhanced with autocompletion, multiselection, custom renderers, and infinite loading.",
  "private": false,
  "author": "RaphaÃ«l Morineau <raphael.morineau@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/moebel-de/material-ui-superselectfield.git"
  },
  "homepage": "https://sharlaan.github.io/material-ui-superselectfield",
  "engines": {
    "node": ">=6"
  },
  "main": "lib/SuperSelectField.js",
  "dependencies": {
    "react-infinite": "^0.12.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "flag-icon-css": "^2.8.0",
    "gh-pages": "^1.0.0",
    "material-ui": "^0.19.4",
    "prettier": "^1.11.0",
    "prettier-eslint": "^8.8.1",
    "prop-types": "^15.6.0",
    "react": ">= 15 < 17",
    "react-addons-test-utils": ">= 15 < 17",
    "react-dom": ">= 15 < 17",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.16",
    "react-test-renderer": "^16.1.0",
    "standard": "^10.0.2"
  },
  "peerDependencies": {
    "material-ui": ">= 0.17 < 1",
    "react": ">= 15 < 17",
    "react-dom": ">= 15 < 17"
  },
  "babel": {
    "plugins": [
      "transform-react-remove-prop-types",
      "transform-class-properties",
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 3%",
              "ie 11",
              "safari 8"
            ]
          },
          "modules": "commonjs",
          "debug": false
        }
      ],
      "react"
    ],
    "compact": true,
    "comments": false,
    "minified": true
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "standard"
    ],
    "parser": "babel-eslint",
    "rules": {
      "camelcase": "off",
      "brace-style": "off",
      "no-trailing-spaces": "off",
      "no-multiple-empty-lines": "off",
      "no-unused-vars": "warn"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "buildLib": "babel src/SuperSelectField.js -o lib/SuperSelectField.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint src",
    "test": "react-scripts test --env=jsdom --coverage",
    "release": "npm run buildLib && npm publish",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/App.js",
      "src/SuperSelectField.js"
    ]
  }
}
